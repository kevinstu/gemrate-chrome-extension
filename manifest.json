{
  "name": "gemrate ext",
  "description": "my ext",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./gemrate-16x16.png",
    "32": "./gemrate-32x32.png",
    "48": "./gemrate-48x48.png",
    "128": "./gemrate-128x128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://myslabs.com/*" 
  ]
}